PI_INT = '141592653589793238462643383279502884197169399375105820974944592307816406286208998628034825342117067'

E_INT = '7182818284590452353602874713526624977572470936999595749669676277240766303535475945713821785251664274'

def pi_real(N):
    pi = '3.141592653589793238462643383279502884197169399375105820974944592307816406286208998628034825342117067'
    aproxpi= pi[0:N+2]
    return aproxpi

def e_real(N):
    e = '2.7182818284590452353602874713526624977572470936999595749669676277240766303535475945713821785251664274'
    aproxe= e[0:N+2]
    return aproxe

print(pi_real(6))
print(e_real(7))
